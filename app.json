{
  "expo": {
    "name": "agroconecta",
    "slug": "agroconecta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-escura.png",
    "scheme": "agroconecta",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/images/logo-escura.png", 
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo-escura.png"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo-escura.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-escura.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-maps",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}